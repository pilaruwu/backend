          <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
            <div class="row my-3">
              <div class="col-md-4"><h2>Usuarios</h2></div>
            </div>
            <div class="mt-2">
                <a class="btn btn-primary w-10" href="http://localhost:9000/addUsu">
                    <h6 class="mt-1">
                        <i class="fa-sharp fa-solid fa-user-plus">
                        </i> Agregar usuario
                    </h6>
                </a>
            </div>
            <div class="table-responsive">
              <table id="table" class="table table-striped table-sm">
                <thead>
                  <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Contrase√±a</th>
                    <th scope="col">Empleado</th>
                    <th scope="col">Rol</th>
                    <th scope="col">Estado</th>
                    <th scope="col">Accion</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each result}}
                  <tr>
                    <th scope="row">{{this.idUsu}}</th>
                    <td>{{this.contrasena}}</td>
                    <td>{{this.nombreEmp}}</td>
                    <td>{{this.nombreRol}}</td>
                    <td>
                        <div class="form-check form-switch">
                            <input class="form-check-input status" type="checkbox" id="flexSwitchCheckDefault" data-id="{{this.idUsu}}" value="{{this.estado}}" {{switchStatus this.estado}}>
                        </div>
                    </td>
                    <td>
                      <a class="btn bg-secondary text-white" href="/userEdit/{{this.idUsu}}">
                        Editar 
                        <i class="fa-sharp fa-solid fa-user-pen"></i>
                      </a>
                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
